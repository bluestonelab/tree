{
  "name": "bluestone/tree",
  "description": "Build tree data structure",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "William",
      "email": "work@suppo.fr"
    }
  ],
  "keywords": [
    "tree",
    "data"
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
